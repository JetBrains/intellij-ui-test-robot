<idea-plugin>
  <id>org.intellij.examples.simple.plugin</id>
  <name>SimplePlugin</name>
  <version>1.0</version>
  <vendor url="https://jetbrains.com">JetBrains</vendor>
  <description>Simple plugin with Say hello action</description>
  <change-notes/>
